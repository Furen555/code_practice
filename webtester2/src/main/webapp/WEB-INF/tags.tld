<?xml version="1.0" encoding="UTF-8" ?>
<taglib>
	<tlib-version>l.0</tlib-version>
	<jsp-version>2.3</jsp-version>
	<shortname>tags</shortname>
	<tag>
		<name>static</name>
		<tag-class>it.source.anichkin.tags.StaticTag</tag-class>
		<body-content>empty</body-content>
	</tag>
	<tag>
		<name>attr</name>
		<tag-class>it.source.anichkin.tags.AttrTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>condition</name>
			<required>true</required>
			<type>java.lang.Boolean</type>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>h</name>
		<tag-class>it.source.anichkin.tags.HTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<name>type</name>
			<required>false</required>
			<type>java.lang.String</type>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>loop</name>
		<tag-class>it.source.anichkin.tags.LoopTag</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<name>count</name>
			<required>true</required>
			<type>java.lang.Integer</type>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>uppercase</name>
		<tag-class>it.source.anichkin.tags.UpperCaseTag</tag-class>
		<body-content>scriptless</body-content>
	</tag>
</taglib>